<!-- Modal-->
<div class="modal fade" id="addLocationModal" tabindex="-1" role="dialog" aria-labelledby="LocationModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="LocationModalLabel">Add New Location</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                @{
                    var location = new TV5_VolunteerEventMgmtApp.Models.Location();
                    <partial name="_LocationPartial" model="location" />
                }
            </div>
        </div>
    </div>
</div>